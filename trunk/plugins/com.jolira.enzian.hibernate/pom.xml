	<dependency>
		<groupId>com.jolira.enzian.dependencies</groupId>
		<artifactId>org.hibernate</artifactId>
		<version>3.2.6.ga-001-SNAPSHOT</version>
	</dependency>
    <dependency>
    	<groupId>com.jolira.enzian.dependencies</groupId>
    	<artifactId>org.hibernate.ejb3-persistence</artifactId>
    	<version>1.0.2.GA-001-SNAPSHOT</version>
    </dependency>
    <dependency>
    	<groupId>com.jolira.enzian.dependencies</groupId>
