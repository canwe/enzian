  </parent>

  <properties>
    <bundle.symbolicName>com.jolira.enzian.hibernate</bundle.symbolicName>
    <bundle.namespace>com.jolira.enzian.hibernate</bundle.namespace>
  </properties>

  <modelVersion>4.0.0</modelVersion>
  <groupId>com.jolira.enzian.plugins</groupId>
